  "version": "v0.1.10",
  "scope": "public",
  "description": "YOLOv8 Segmentation Model Application",
  "attributes": {
    "Provider": "Ultralytics",
    "License": "AGPL-3.0",
    "Category": "Model",
    "Computer Vision": "Semantic Segmentation",
    "Media Type": "Image"
  },
  "codebase": {
    "type": "git",
